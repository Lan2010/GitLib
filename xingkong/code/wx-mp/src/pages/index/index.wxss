.award-des {
  position: absolute;
  top: -81rpx;
  width: 100%;
  line-height: 36rpx;
  text-align: center;
  font-size: 24rpx;
}
.award .rule-link {
  position: absolute;
  top: 330rpx;
  width: 100%;
  marign: auto;

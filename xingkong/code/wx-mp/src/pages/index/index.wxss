}

.winner {
  position: absolute;
  top: 19rpx;
  left: 0;
  right: 0;
  width: 662rpx;
  height: 303rpx;
  margin: auto;
}
.winner .title {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  overflow: hidden;
}
.winner .title .name {
  float: left;
  padding: 0 0 0 20rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #feee7a;
}
.winner .title .more {
  float: right;
  display: block;
  height: 100%;
  padding: 0 19rpx;
  box-sizing: border-box;
  font-size: 24rpx;
}
.winner .list {
  position: absolute;
  top: 80rpx;
  left: 0;
  width: 100%;
  height: 229rpx; 
  overflow: hidden;
  box-sizing: border-box;
}
.winner .list-inner {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 240rpx;
}
.winner .list-scroll {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
}
.winner .list-scroll.transition {
  -webkit-transition: all .3s linear;
    transition: all .3s linear;
}
.winner .list-item {
  display: flex;
  align-items: center; 
  float: left;
  width: 100%;
  height: 80rpx;
  /* line-height: 70rpx; */
  text-align: left;
  font-size: 24rpx;
  box-sizing: border-box;
  /* border-bottom: 1px solid red; */
}
.winner .list-item .hd {
  display: block;
  height: 50rpx;
  padding-left: 20rpx;
}
.winner .list-item .bd {
  flex: 1;
  padding-left: 10rpx;
}
.winner .list-item .ft {
  padding-right: 20rpx;
}
.winner .list-item .bd,
.winner .list-item .ft {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.winner .list-item image {
  width: 50rpx;
  height: 50rpx;
  border: 1rpx solid #fff;
  border-radius: 50%; 
}


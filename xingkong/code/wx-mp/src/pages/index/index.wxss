/* Popup - rules */

.popup-rule {
  
}
.rules-mask {
  position: fixed;
  z-index: 1;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  /*border: 1px solid red;*/
  background: rgba(0, 0, 0, 0.6);
}
.popup-rule .rules {
  position: fixed;
  top: 50%;
  left: 0;
  right: 0;
   z-index: 2; 
  overflow: hidden;
  width: 600rpx;
  margin: auto;
  line-height: 34rpx;
  color: rgb(48, 48, 48);
  background-color: #fff;
  border-radius: 20rpx;
  -webkit-transform: translate(0, -50%);
  transform: translate(0, -50%);
}

.rules {

}
.rules ul {
  padding: 40rpx 40rpx 40rpx 65rpx;
}
.rules li {
  position: relative;
  padding-bottom: 30rpx;
  line-height: 44rpx;
  font-size: 28rpx;
  word-break: break-all;
}
.rules li:last-child {
  padding-bottom: 0;
}
.rules .t {
  position: relative;
  padding-bottom: 10rpx;
  font-weight: bold;
}
.rules .t::after {
  content: ' ';
  position: absolute;
  top: 16rpx;
  left: -25rpx;
  width: 12rpx;
  height: 12rpx;
  marign: auto;
  background-color: gray;
  border-radius: 50%;
}
.rules .d {
  position: relative;
  padding-left: 34rpx;
}
.rules .d.n {
  padding-left: 0;
}
.rules .d i {
  position: absolute;
  top: 0;
  left: 0;
}
.rules button {
  font-size: 30rpx;
  color: #666;
  border-radius: 0 0 20rpx 20rpx;
}


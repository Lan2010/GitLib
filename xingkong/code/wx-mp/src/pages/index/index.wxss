/**index.wxss**/


/* Tab */

.tab-page {
  width: 100%;
  min-height: 100%; 
   /* padding-bottom: 98rpx;  */
  /* border: 1px solid red; */
}

.tab-content {
  position: relative; 
  width: 100%;
  padding-bottom: 110rpx;
  word-break: break-all;
  /* border: 1px solid red; */
}

.fixedLowHeight {
  padding-bottom: 124rpx;
}

.tab {
  /* display: none; */
  display: flex;  
  align-items: center;
  justify-content: center;

  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  height: 98rpx;
  /* border-top: 1rpx solid #efeff4; */
  background-color: rgba(0, 0, 0, 0.1);
}
.tab .item {
  flex: 1;
  text-align: center;
}
.tab .item .icon {
  height: 48rpx;
  font-size: 42rpx;
  line-height: 48rpx;
  color: #7b7e82;
   /* background-color: red;  */
}
.tab .item .icon image {
  width: 58rpx;
  height: 48rpx;
}
.tab .item text {
  display: block;
  padding-top: 7rpx; 
  line-height: 20rpx;
  font-size: 20rpx;
  color: #fff;
}
.tab .item.active .icon,
.tab .item.active text {
  color: #00edf0;
}

.tab.black {
  background-color: rgba(255, 255, 255, 0.3);
}
.tab.black text {
  color: rgb(48, 48, 48);
}

.iosX .tab {
  height: 166rpx;
  box-sizing: border-box;
  padding-top: 20rpx;
  padding-bottom: 68rpx;
}


/** Page - User **/

.page-user .tab-content {
  position: relative;
  background-color: #f2f2f2;
}

.user-info-space {
  width: 100%;
  height: 480rpx;
}

.user-info {
  position: fixed;
   top: 0;
  left: 0;
  z-index: 1;
  width: 100%;
  height: 480rpx;
  text-align: center;
}

.user-info-bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 480rpx;
}

.user-info .inner {
   padding-top: 35rpx; 
}

/* .user-head, */

.user-info button {
  display: inline-block;
  margin: auto;
  width: 400rpx;
  height: 280rpx; 
  padding: 0;
  /* border: 1px solid red; */
  background: none;
}
.user-head {
  position: relative;
  width: 186rpx;
  height: 186rpx;
  margin: auto;
  background: rgba(255, 255, 255, .5);
  border-radius: 50%;
}
.user-head image {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: block;
  width: 168rpx;
  height: 168rpx;
  margin: auto;
  border-radius: 50%;
  opacity: 1;
}
.user-nickname {
  padding-top: 30rpx; 
  line-height: 28rpx;
  font-size: 32rpx;
  /* color: #4bfdff; */
  color: #fff;
}


